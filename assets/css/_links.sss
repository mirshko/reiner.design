h2 a
  border-bottom-width: 4px !important

a:not(.logo)
  color: var(--link)
  text-decoration: none
  outline: none
  font-weight: 600

  &:after
    margin-left: 4px
    font-family: "Fira Code"
    content: '->'

  &:hover,
  &:focus,
  &:active
    color: color(var(--white))
    background-color: var(--link)
